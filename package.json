{
  "name": "mtg-tourney-app",
  "version": "0.1.0",
  "description": "A server which receives pdfs of Player Pairings, and displays that data in mobile friendly, easy-to-check-from-your-seat way. No more stampedes to the pairings sheet!",
  "main": "server.js",
  "scripts": {
    "test": "node test/*.js",
    "build": "browserify client/index.js -g uglifyify -o public/bundle.js -dv"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mixmix/mtg-tourney-app.git"
  },
  "keywords": [
    "magic",
    "mtg",
    "tournament"
  ],
  "author": "mixmix",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "uglifyify": "^3.0.2"
  },
  "dependencies": {
    "express": "^4.14.0",
    "formidable": "^1.0.17",
    "mtg-tourney-parser": "^0.1.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  }
}
